instruments {
    Sine {
        frequency: Float;

        init(freq: Float) {
            print("Frequency: ");
            println(freq);
            frequency = freq;
        }

        perf(amps: Float) {
            print("Frequency: ");
            println(frequency);

            print("Amps: ");
            println(amps);

            local name: String = "Hello";
            println(name);
        }
    }

    Dummy {
        init() {
            println("Dummy");
        }
    }
}

score {
    Dummy(0.0 0.1);
    
    Sine(0.0 0.25 init(440.0) perf(1.0));
}